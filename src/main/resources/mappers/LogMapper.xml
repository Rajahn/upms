<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hr.mapper.LogMapper">

    <insert id="saveLog" parameterType="com.hr.entity.SysLog">
     INSERT INTO SYS_LOG(USERNAME,OPERATION,TIME,METHOD,PARAMS,IP,CREATE_TIME) VALUE
      ( #{username}, #{operation}, #{time}, #{method}, #{params}, #{ip}, #{createTime})
    </insert>

</mapper>